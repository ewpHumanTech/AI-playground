import plotly.graph_objects as go
import pandas as pd

# Load data from CSV
df = pd.read_csv("data.csv")

# Create traces
fig = go.Figure()

for col in df.columns[1:]:
    fig.add_trace(go.Scatter(x=df['Date'], y=df[col], mode='lines+markers', name=col))

fig.update_layout(title='Subscriber Data',
                  xaxis_title='Date',
                  yaxis_title='Count')
fig.show()
